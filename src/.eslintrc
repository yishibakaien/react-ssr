{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": "airbnb",
  "rules": {
    "semi": [0],
    "arrow-parens": [0],
    "react/jsx-filename-extension": [0],
    "comma-dangle": [0],
    "class-methods-use-this": [0],
    "space-before-function-paren": [0],
    "padded-blocks": [0],
    "no-console": [0],
    "react/destructuring-assignment": [0],
    "no-trailing-spaces": [0],
    "react/require-default-props": [0]
  }
}
